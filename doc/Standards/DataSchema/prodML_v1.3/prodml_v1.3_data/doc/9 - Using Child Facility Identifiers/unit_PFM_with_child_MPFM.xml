							<flow>production</flow>
							<product>oil and gas</product>
							<qualifier>measured</qualifier>
						</expectedFlowProduct>
					</expectedFlowProperty>
				</port>
				<!-- outlet port -->
				<port uid="2">
					<name>D-E outlet</name>
					<direction>outlet</direction>
					<!-- The facility -->
					<facility kind="flow meter" siteKind="multi-phase flow meter" uidRef="MPFM01">Multi-phase flow meter #1</facility>
					<!-- Connected to separator unit E -->
					<connectedNode>
						<node>D-E</node>
						<dTimStart>2012-02-01T01:00:00+03:00</dTimStart>
					</connectedNode>
					<!-- measured production for combined (oil/gas/water) flow -->
					<expectedFlowProduct>
						<flow>production</flow>
						<product>oil and gas</product>
						<qualifier>measured</qualifier>
					</expectedFlowProduct>
					<!-- measured production for gas flow -->
					<expectedFlowProduct>
						<flow>production</flow>
						<product>gas</product>
						<qualifier>measured</qualifier>
					</expectedFlowProduct>
					<!-- gas flow rate -->
					<expectedFlowProperty>
						<property>flow rate</property>
						<dTimStart>2012-02-01T01:00:00+03:00</dTimStart>
						<expectedFlowProduct>
							<flow>production</flow>
							<product>gas</product>
							<qualifier>measured</qualifier>
						</expectedFlowProduct>
					</expectedFlowProperty>
				</port>
			</unit>
		</network>
	</productFlowModel>
</productFlowModels>
