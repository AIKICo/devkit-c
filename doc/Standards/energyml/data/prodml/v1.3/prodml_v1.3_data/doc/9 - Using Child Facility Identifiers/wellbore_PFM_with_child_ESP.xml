<?xml version="1.0" encoding="UTF-8"?>
<!-- Saudi Aramco CR-0012 Flow Network Example using childFacilityIdentifier -->
<productFlowModels
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://www.prodml.org/schemas/1series"
	xsi:schemaLocation="http://www.prodml.org/schemas/1series ../../schemas/prodml_v1.2.1_data/xsd_schemas/obj_productFlowModel.xsd"
	version="1.2.0.0(PRODML)">
	<productFlowModel uid="PFM-01">
		<name>Saudi Aramco CR-0012 Flow Network Example</name>
		<dTimStart>2012-01-01T00:00:00.000Z</dTimStart>
		<network uid="NET-B01">
			<name>Wellbore B01 Internal Network</name>
			<!-- Dammam #1 wellbore unit -->
			<unit uid="U-B01">
				<name>Dammam #1 wellbore unit</name>
				<facility kind="wellbore" uidRef="B01">Dammam #1 wellbore</facility>
				<facilityParent1 kind="well" uidRef="W01">Dammam #1</facilityParent1>
				<contextFacility kind="field" uidRef="F01">Ghawar</contextFacility>
				<!-- ESP motor current -->
				<expectedProperty>
					<property>motor current</property>
					<childFacilityIdentifier>electric submersible pump(ESP01)</childFacilityIdentifier>
				</expectedProperty>
				<!-- bottomhole pressure -->
				<expectedProperty>
					<property>pressure</property>
					<childFacilityIdentifier>bottomhole</childFacilityIdentifier>
				</expectedProperty>
				<!-- Inlet port from reservior -->
				<port uid="1">
					<name>RESERVOIR-B01 inlet</name>
					<direction>inlet</direction>
					<connectedNode>
						<node>RESERVOIR</node>
						<dTimStart>2012-02-01T01:00:00+03:00</dTimStart>
					</connectedNode>
				</port>
				<!-- Outlet port to wellhead -->
				<port uid="2">
					<name>B01-HDR01 outlet</name>
					<direction>outlet</direction>
					<connectedNode>
						<node>B01-HDR01</node>
						<dTimStart>2012-02-01T01:00:00+03:00</dTimStart>
					</connectedNode>
				</port>
			</unit>
		</network>
	</productFlowModel>
</productFlowModels>
