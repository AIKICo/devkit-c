	<xsd:simpleType name="LogDataType">
		<xsd:annotation>
			<xsd:documentation>The endcoding allowed in a realtime channel value or log curve value.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="witsml:abstractTypeEnum">
