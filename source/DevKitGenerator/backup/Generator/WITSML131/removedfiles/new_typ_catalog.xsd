	<!--                                                         --> 
	<xsd:simpleType name="LithologyType">
		<xsd:annotation>
			<xsd:documentation>The type of lithology.
			The list of standard values is contained in the WITSML enumValues.xml file. </xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="witsml:abstractTypeEnum">
			<xsd:enumeration value="andesite">
				<xsd:annotation>
					<xsd:documentation>Intermediate porphyritic volcanic rock consisting of plagioclase, pyroxene, hornblende and bitotite.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="anhydrite">
				<xsd:annotation>
					<xsd:documentation>The mineral form of anhydrous calcium sulfate, commonly massive in evaporite beds.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="arkose">
				<xsd:annotation>
					<xsd:documentation>An indurated arenaceous deposit consisting chiefly of quartz and feldspar (&gt;25%).</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="basalt">
				<xsd:annotation>
					<xsd:documentation>Volcanic rock consisting essentially of plagioclase feldspar and pyroxene.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="breccia">
				<xsd:annotation>
					<xsd:documentation>A lithified rock rich in angular fragments of size greater than 2mm.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="calcarenite">
				<xsd:annotation>
					<xsd:documentation>A limestone more than half of which consists of cemented sand-size grains of calcium carbonate: a consolidated calcareous sand.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="calcilutite">
				<xsd:annotation>
					<xsd:documentation>A limestone more than half of which consists of detrital calcite particles of silt and/or clay size: a consolidated calcareous mud.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="calcisiltite">
				<xsd:annotation>
					<xsd:documentation>A limestone more than half of which consists of detrital calcite particles of silt size: a consolidated calcareous silt.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="chalk">
				<xsd:annotation>
					<xsd:documentation>A fine-grained limestone composed largely of coccolith fragments.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="chert">
				<xsd:annotation>
					<xsd:documentation>Siliceous sedimentary rock containing finely crystalline snd fibrous quartz.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="clay">
				<xsd:annotation>
					<xsd:documentation>Unconsolidated deposit of very fine particles of size generally less than 1/256mm and composed of clay minerals.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="claystone">
				<xsd:annotation>
					<xsd:documentation>An indurated clay, lacking the fissility of shale.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="coal">
				<xsd:annotation>
					<xsd:documentation>A combustible organic sedimentary rock with less than 40% of mineral matter (based on dry material) composed of polymers of cyclic hydrocarbons.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="conglomerate">
				<xsd:annotation>
					<xsd:documentation>An indurated sedimentary rock containing an essential quantity of rounded rock fragments larger than 2mm, and normally having a matrix of sand.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="diabase">
				<xsd:annotation>
					<xsd:documentation>Igneous rock of grain size intermediate between basalt and gabbro composed of plagioclase feldspar, pyroxene and opaque minerals. Synonym: dolerite.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="diorite">
				<xsd:annotation>
					<xsd:documentation>Plutonic igneous rock consisting of plagioclase feldspar, commonly with hornblende and biotite.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="dolomite">
				<xsd:annotation>
					<xsd:documentation>Sedimentary rock containing more than 90% dolomite (mineral) and less than 10% calcite.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="dolomite, calcareous">
				<xsd:annotation>
					<xsd:documentation>A dolomite rock containing more than 10% calcite.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="extrusive rock (volcanic)">
				<xsd:annotation>
					<xsd:documentation>Igneous rocks formed by solidification of material erupted sub-aerially or sub-aqueously onto the earth's surface, as either lava flows or pyroclastic material.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="feldspar">
				<xsd:annotation>
					<xsd:documentation>The family of aluminium silicates of sodium, potassium, calcium and other metals which comprise over 60% of the earth's crust, are the most abundant mineral group, and occur in all types of rocks.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="gabbro">
				<xsd:annotation>
					<xsd:documentation>Coarse grained plutonic igneous rock composed essentially of calcic plagioclase feldspar, pyroxenes and iron oxide.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="glauconite">
				<xsd:annotation>
					<xsd:documentation>A mineral composed of ferrous potassium silicates related to mica which commonly imparts a green color to its sedimentary host rock.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="gneiss">
				<xsd:annotation>
					<xsd:documentation>A metamorphic rock, commonly foliated and crystalline, whose mineral content can range from quartz and feldspars to amphiboles and pyroxenes.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="granite">
				<xsd:annotation>
					<xsd:documentation>Coarse grained plutonic rock typically containing free quartz, alkali feldspar and biotite mica.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="gravel">
				<xsd:annotation>
					<xsd:documentation>An unconsolidated deposit of coarse clastics. The diameters on the Wenworth scale are generally greater than 2mm.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="greenstones">
				<xsd:annotation>
					<xsd:documentation>A field term for any dark fine grained altered or metamorphosed basic igneous rock.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="greywacke">
				<xsd:annotation>
					<xsd:documentation>A dark grey consolidated deposit containing feldspar, rock fragments and a dark chloritic matrix.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="gumbo">
				<xsd:annotation>
					<xsd:documentation>A term used in the U.S. for clay soils that become sticky, impervious, and plastic when wet.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="gypsum">
				<xsd:annotation>
					<xsd:documentation>A mineral composed of hydrous calcium sulphate, commonly forming thick extensive evaporite beds.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="halite">
				<xsd:annotation>
					<xsd:documentation>A mineral composed of sodium chloride common in evaporite rocks.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="igneous">
				<xsd:annotation>
					<xsd:documentation>Term applied to rock or mineral formed by solidification of molten or partly molten material (magma).</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="intrusvie rock (plutonic)">
				<xsd:annotation>
					<xsd:documentation>Igneous rock formed by cooling and solidification of magma within the earth's crust.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="lignite">
				<xsd:annotation>
					<xsd:documentation>A low-rank humic coal.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="limestone">
				<xsd:annotation>
					<xsd:documentation>A sedimentary rock composed mostly of the mineral calcite (calcium carbonate) which is the consolidated equivalent of lime mud, calcareous sand, and/or shell fragments.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="limestone, argillaceous">
				<xsd:annotation>
					<xsd:documentation>A limestone homogeneously mixed with 10-50% clay minerals.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="limestone, dolomitic">
				<xsd:annotation>
					<xsd:documentation>A limestone containing 10-50% dolomite and 50-90% calcite.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="limestone, sandy">
				<xsd:annotation>
					<xsd:documentation>A limestone homogeneously mixed with 10-50% quartz grains.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="marble">
				<xsd:annotation>
					<xsd:documentation>A metamorphic rock composed of calcite.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="marl">
				<xsd:annotation>
					<xsd:documentation>A consolidated or semi-consolidated mixture of clay and generally fine grained carbonate material.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="metamorphic rocks">
				<xsd:annotation>
					<xsd:documentation>Rocks formed by mineralogical, chemical and structural alteration of pre-existing rocks under conditions of temperature and pressure at depth in the earth.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="mudstone">
				<xsd:annotation>
					<xsd:documentation>Consolidated and indurated mud.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="no description">
				<xsd:annotation>
					<xsd:documentation>No description is provided.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="no sample">
				<xsd:annotation>
					<xsd:documentation>No sample is provided.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ophiolite">
				<xsd:annotation>
					<xsd:documentation>Basic and ultra-basic igneous rocks and their metamorposed, often hydrated, derivatives, believed to represent former oceanic crust.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="peat">
				<xsd:annotation>
					<xsd:documentation>Unconsolidated, semi-carbonised plant remains which are a precursor of humic coal.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="phosphate">
				<xsd:annotation>
					<xsd:documentation>Any mineral compound containing the ((PO4)3) radical.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="phosphate rock">
				<xsd:annotation>
					<xsd:documentation>Any rock containing phosphate minerals such as apatite of sufficient purity and abundance to be an economic resource.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="potassium and magnesium salts">
				<xsd:annotation>
					<xsd:documentation>Salts such as sylvinite, carnalite, keiserite with swelling properties that can severely obstruct drilling operations.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="quartzite">
				<xsd:annotation>
					<xsd:documentation>A sedimentary rock composed of more than 95% of quartz.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="rhyolite">
				<xsd:annotation>
					<xsd:documentation>Silica rich volcanic rock consisting of quartz and alkali feldspar with minor plagioclase and biotite.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="salt">
				<xsd:annotation>
					<xsd:documentation>Common term for naturally occurring sodium chloride.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="sand">
				<xsd:annotation>
					<xsd:documentation>An aggregation of particles having diameters in the range 1/16mm to 2mm usually consisting of quartz.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="sandstone">
				<xsd:annotation>
					<xsd:documentation>A sedimentary rock composed of indurated sand.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="schist">
				<xsd:annotation>
					<xsd:documentation>A strongly foliated crystalline metamorphic rock.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="serpentine">
				<xsd:annotation>
					<xsd:documentation>A rock forming mineral of hydrous iron and magnesium silicates, often resulting from metamorhism of amphiboles and pyroxenes.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="shale">
				<xsd:annotation>
					<xsd:documentation>A fine-grained sedimentary rock formed by the compaction of clay, silt or mud.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="silt">
				<xsd:annotation>
					<xsd:documentation>Unconsolidated sedimentary material with grain sizes between 1/16mm and 1/256mm.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="siltstone">
				<xsd:annotation>
					<xsd:documentation>Consolidated silt.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="slate">
				<xsd:annotation>
					<xsd:documentation>A compact fine-grained metamorphic rock displaying slaty cleavage along which the rock may be cut into sheets.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="syenite">
				<xsd:annotation>
					<xsd:documentation>Plutonic igneous rock composed essentially of alkali feldspar, with lesser sodic plagioclase, biotite and hornblende, and minor quartz.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="tillite (diamictite)">
				<xsd:annotation>
					<xsd:documentation>Consolidated unsorted glacial deposits with particles ranging from boulder to mud in grain size and of highly variable composition.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="trachyte">
				<xsd:annotation>
					<xsd:documentation>A group of fine-grained often porphyritic volcanic rocks containing alkali feldspar and minor mafic minerals.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="tuff">
				<xsd:annotation>
					<xsd:documentation>An indurated pyroclastic deposit, predominantly consisting of fine-grained volcanic rocks: sedimentary particles may be present. Tuff may be deposited sub-aerially or sub-aqueously, and may be well sorted or heterogeneous.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ultrabasic">
				<xsd:annotation>
					<xsd:documentation>A class of igneous rocks composed mostly of mafic minerals such as pyroxenes along with calcic feldspar, with no free silica and a total silica content less than about 45%.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="undifferentiated">
				<xsd:annotation>
					<xsd:documentation>The lithology is not differentiated. This is typically used to fill 
			out a lithology description to 100 percent.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="unknown">
				<xsd:annotation>
					<xsd:documentation>The value is not known. This value should not be used
			in normal situations. All reasonable attempts should be made to determine
			the appropriate value. Use of this value may result in rejection in 
			some situations.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
